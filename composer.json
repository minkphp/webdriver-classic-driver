{
    "name": "mink/webdriver-classic-driver",
    "description": "W3C WebDriver Classic driver for Mink framework",
    "keywords": ["selenium", "webdriver", "javascript", "ajax", "testing", "browser"],
    "homepage": "https://mink.behat.org/",
    "type": "mink-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Sciberras",
            "email": "christian@sciberras.me",
            "homepage": "https://github.com/uuf6429"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "behat/mink": "^1.11@dev",
        "php-webdriver/webdriver": "^1.14"
    },
    "require-dev": {
        "mink/driver-testsuite": "dev-master",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9.6.8",
        "symfony/error-handler": "^5.4 || ^6.0",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mink\\WebdriverClassDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mink\\WebdriverClassDriver\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "scripts": {
        "lint": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phpstan"
        ],
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phpunit"
        ]
    }
}
